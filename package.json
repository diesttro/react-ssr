{
  "name": "react-ssr",
  "version": "1.0.0",
  "scripts": {
    "build:client": "webpack --config webpack.client.js --mode production",
    "build:server": "webpack --config webpack.server.js --mode production",
    "build": "run-s clean build:*",
    "clean": "rimraf dist",
    "dev:client": "webpack --config webpack.client.js --mode development --watch",
    "dev:server": "webpack --config webpack.server.js --mode development --watch",
    "dev": "run-p dev:*",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "express": "^4.17.1",
    "modern-normalize": "^1.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.1.3",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "ignore-loader": "^0.1.2",
    "mini-css-extract-plugin": "^1.3.9",
    "nodemon": "^2.0.7",
    "nodemon-webpack-plugin": "^4.4.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "webpack": "^5.27.0",
    "webpack-cli": "^4.5.0",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^2.5.2"
  }
}
